<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students</title>
</head>
<body>
    <h2>Students</h2>
    <a href="/add">Add Student</a>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Notes</th>
        </tr>
        {{#each students}}
            <tr>
                <td>{{this.ID}}</td>
                <td>{{this.NAME}}</td>
                <td>{{this.AGE}}</td>
                <td>{{this.NOTES}}</td>
            </tr>
        {{/each}}
    </table>
</body>
</html>